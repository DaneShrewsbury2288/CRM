{
  "name": "crm",
  "version": "1.0.0",
  "engines": {
    "node": "10.3.0"
  },
  "description": "CRM",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install && npm run build && cd ..",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Dane Shrewsbury",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint": "^6.3.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-lineheight-annotation": "^0.3.5",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "google-maps-react": "^2.0.2",
    "http-proxy-middleware": "^0.19.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "material-table": "^1.50.0",
    "material-ui": "^0.20.2",
    "material-ui-pickers": "^2.2.4",
    "moment": "^2.24.0",
    "mongoose": "^5.6.11",
    "mongoose-auto-increment": "^5.0.1",
    "mui-datatables": "^2.8.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react-chartjs-2": "^2.7.6",
    "react-google-maps": "^9.4.5",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "react-spinners": "^0.6.1",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.3.0",
    "react-trello": "^2.2.3",
    "recharts": "^1.7.1",
    "redux": "^4.0.4",
    "socket.io": "^2.2.0",
    "validator": "^11.1.0"
  }
}
